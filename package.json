{
  "name": "see_pdfeditor",
  "version": "2.1.0",
  "description": "Multi-PDF editor with image insertion - Secure, client-side PDF editing",
  "private": true,
  "main": "production-build/index.html",
  "scripts": {
    "dev": "cd production-build && python -m http.server 5173",
    "build:css": "tailwindcss -i ./production-build/input.css -o ./production-build/output.css --minify",
    "watch:css": "tailwindcss -i ./production-build/input.css -o ./production-build/output.css --watch",
    "test": "echo \"No tests configured yet\" && exit 0",
    "build": "npm run build:css",
    "lint": "eslint . --ext .js --max-warnings 0",
    "lint:fix": "eslint . --ext .js --fix",
    "benchmark": "echo 'Benchmark scripts moved to docs/'",
    "generate-sri": "echo 'SRI generation not needed for production'",
    "link-check": "markdown-link-check -c .mlc.json",
    "docs:build": "mkdocs build",
    "docs:serve": "mkdocs serve"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/aeimonitors/see_pdfeditor.git"
  },
  "keywords": [
    "pdf",
    "editor",
    "multi-pdf",
    "image-to-pdf",
    "annotation",
    "client-side",
    "security"
  ],
  "author": "aeimonitors",
  "license": "MIT",
  "bugs": {
    "url": "https://github.com/aeimonitors/see_pdfeditor/issues"
  },
  "homepage": "https://github.com/aeimonitors/see_pdfeditor#readme",
  "devDependencies": {
    "autoprefixer": "^10.4.21",
    "daisyui": "^5.3.10",
    "eslint": "^8.50.0",
    "eslint-config-airbnb-base": "^15.0.0",
    "eslint-plugin-import": "^2.28.0",
    "markdown-link-check": "3.10.3",
    "postcss": "^8.5.6",
    "tailwindcss": "^4.1.16"
  },
  "engines": {
    "node": ">=18.0.0",
    "npm": ">=9.0.0"
  }
}
